{
  "name": "svelte-starter-kit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "eslint": "eslint ./src/**/*.{js,svelte} --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "cables": "node cables.cjs"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-vercel": "2.3.1",
    "@sveltejs/kit": "^1.5.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-abbr": "^1.4.1",
    "remark-github": "^11.2.4",
    "svelte": "^3.57.0",
    "svelte-check": "^3.0.1",
    "svelte-loader": "^3.1.7",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4"
  },
  "type": "module",
  "dependencies": {
    "@cables/cables": "^0.11.0",
    "@vercel/kv": "^0.2.2",
    "daisyui": "^2.51.4",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-svelte": "^2.23.1",
    "i": "^0.3.7",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.9",
    "svelte-inview": "^4.0.0",
    "svelte-meta-tags": "^2.8.0",
    "svelte-spa-router": "^3.3.0"
  }
}
